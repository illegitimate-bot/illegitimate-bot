{
    "name": "illegitimate-bot",
    "displayName": "Illegitimate Bot",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.ts",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/illegitimate/illegitimate-bot"
    },
    "homepage": "https://gitlab.com/illegitimate/illegitimate-bot",
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "watch": "tsc -w",
        "dev": "conc --names \"TSC,BOT\" --prefix-colors \"blue, magenta\" \"tsc -w\" \"yarn dev:js\"",
        "dev:ts": "nodemon --config nodemon.json src/index.ts",
        "dev:js": "nodemon --config nodemon-js.json dist/index.js",
        "dev:build": "ts-node scripts/dev-deploy.ts",
        "dev:delete": "ts-node scripts/delete-commands.ts",
        "lint": "eslint src",
        "lint:fix": "eslint --fix src",
        "prod:build": "ts-node scripts/deploy-commands.ts"
    },
    "author": "Taken",
    "license": "GPL-3.0-only",
    "dependencies": {
        "@discord-player/extractor": "^4.4.6",
        "@evan/opus": "^1.0.3",
        "anilist": "^0.12.3",
        "axios": "^1.6.7",
        "chalk": "^4.1.2",
        "cron": "^3.1.6",
        "discord-player": "^6.6.7",
        "discord.js": "^14.14.1",
        "dotenv": "^16.4.5",
        "ioredis": "^5.3.2",
        "mongoose": "^8.1.3",
        "ms": "^2.1.3",
        "pretty-ms": "^8.0.0",
        "youtube-ext": "^1.1.16"
    },
    "devDependencies": {
        "@types/ms": "^0.7.34",
        "@types/node": "^20.11.19",
        "@typescript-eslint/eslint-plugin": "^7.0.2",
        "@typescript-eslint/parser": "^7.0.2",
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "packageManager": "yarn@4.1.0"
}
