{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "illegitimate-bot",
    "displayName": "Illegitimate Bot",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.ts",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/illegitimate/illegitimate-bot"
    },
    "homepage": "https://gitlab.com/illegitimate/illegitimate-bot",
    "scripts": {
        "start": "node dist/index.js",
        "build": "swc src -d dist --strip-leading-paths",
        "watch": "swc src -d dist --strip-leading-paths --watch",
        "dev": "nodemon --config nodemon-swc.json",
        "dev:ts": "nodemon --config nodemon.json src/index.ts",
        "dev:js": "nodemon --config nodemon-js.json dist/index.js",
        "format": "dprint fmt \"**/*.ts\"",
        "check": "yarn tscheck && yarn lint",
        "tscheck": "tsc && echo \"TypeScript check passed!\"",
        "lint": "eslint && echo \"Linting passed!\"",
        "lint:fix": "eslint --fix",
        "push:prod": "tsx scripts/deploy-commands.ts",
        "push:dev": "tsx scripts/dev-deploy.ts",
        "delete:dev": "tsx scripts/delete-commands.ts"
    },
    "author": "Taken",
    "license": "GPL-3.0-only",
    "dependencies": {
        "@discord-player/extractor": "^4.5.1",
        "@evan/opus": "^1.0.3",
        "anilist": "^0.12.4",
        "axios": "^1.7.7",
        "chalk": "^5.3.0",
        "cron": "^3.1.7",
        "discord-player": "^6.7.1",
        "discord-player-youtubei": "^1.3.1",
        "discord.js": "^14.16.2",
        "dotenv": "^16.4.5",
        "ioredis": "^5.4.1",
        "mongoose": "^8.6.3",
        "ms": "^2.1.3",
        "pg": "^8.13.0",
        "pg-hstore": "^2.3.4",
        "pretty-ms": "^9.1.0",
        "sequelize": "^6.37.3"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.11.0",
        "@swc/cli": "^0.4.0",
        "@swc/core": "^1.7.26",
        "@types/ms": "^0.7.34",
        "@types/node": "^22.5.5",
        "@types/pg": "^8",
        "@typescript-eslint/eslint-plugin": "^8.6.0",
        "@typescript-eslint/parser": "^8.6.0",
        "chokidar": "^4.0.0",
        "dprint": "^0.47.2",
        "eslint": "^9.11.0",
        "globals": "^15.9.0",
        "sqlite3": "^5.1.7",
        "tsx": "^4.19.1",
        "typescript": "^5.6.2"
    },
    "packageManager": "yarn@4.5.0"
}
